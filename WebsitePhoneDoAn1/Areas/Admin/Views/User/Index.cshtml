
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using Microsoft.AspNetCore.Identity
@model IEnumerable<IdentityUser>

<table class="table">
    <thead>
        <tr>
            <td>Id</td>
            <td>User name</td>
            <td>Email</td>
         
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.UserName</td>
                <td>@item.Email</td>
                              
            </tr>
        }
    </tbody>
</table>



